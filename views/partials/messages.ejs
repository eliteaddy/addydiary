<% if(typeof errors != 'undefined') { %>
    <% errors.forEach(function(error) {%>
        <div class="text-center text-danger small three">
            <%= error.msg %>
        </div>
    <% }); %>
<% } %>

<% if(success_msg != '') { %>
    <div class="text-center text-success small">
        <%= success_msg %>
    </div>
<% } %>

<% if(error_msg != '') { %>
    <div class="text-center text-danger small one">
        <%= error_msg %>
    </div>
<% } %>

<% if(error != '') { %>
    <div class="text-center text-danger small two">
        <%= error %>
    </div>
<% } %>